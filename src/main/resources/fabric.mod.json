    "license": "ComputerCraft Public License",
    "icon": "assets/computercraft/pack.png",
    "contact": {
        "homepage": "https://github.com/cc-tweaked/cc-restitched",
        "issues": "https://github.com/cc-tweaked/cc-restitched/issues"
    },
    "authors": [
        "Daniel Ratcliffe",
        "Aaron Mills",
        "SquidDev",
        "parly",
        "Merith.TK",
